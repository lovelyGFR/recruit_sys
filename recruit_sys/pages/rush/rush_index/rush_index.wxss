/* footer */
/* 倒计时 */
.countdown{
    display: flex;
    flex-direction: row;
    align-items: center;
    height: 84rpx;
    margin-top: 30rpx;
    margin-left: 20rpx;
    margin-right: 20rpx;
}
.countdown .radiusleft{
    width: 84rpx;
    height: 84rpx;
    border-top-left-radius: 100%;
    border-bottom-left-radius: 100%;
    background-color: #ffffff;
}
.countdown .center{
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    color: #54AA38;
    font-size: 30rpx;
    width: 542rpx;
    height: 84rpx;
    background-color: #ffffff;   
}
.countdown .radiusright{
    width: 84rpx;
    height: 84rpx;
    border-top-right-radius: 42rpx;
    border-bottom-right-radius: 42rpx;
    background-color: #ffffff;
}

/* 抢票按钮 */
.button{
    line-height: 84rpx;
    height: 84rpx;
    width: 710rpx;
    margin-top: 30rpx;
    margin-left: 20rpx;
    margin-right: 20rpx;
    border-radius: 42rpx;
    /* background-color:#52A9DA; */
    background: linear-gradient(to right, #49ADD6, #7A94ED);
    font-size: 30rpx;
    color: #fff;
}
.button-hover{
    opacity: 0.9;
}

/* 抢票结束 */
.end_tickets{
    display: flex;
    flex-direction: row;
    align-items: center;
    height: 84rpx;
    margin-top: 30rpx;
    margin-left: 20rpx;
    margin-right: 20rpx;
}
.end_tickets .radiusleft{
    width: 84rpx;
    height: 84rpx;
    border-top-left-radius: 100%;
    border-bottom-left-radius: 100%;
    background-color: #ffffff;
}
.end_tickets .center{
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    color: #AAAAAA;
    font-size: 30rpx;
    width: 542rpx;
    height: 84rpx;
    background-color: #ffffff;   
}
.end_tickets .radiusright{
    width: 84rpx;
    height: 84rpx;
    border-top-right-radius: 42rpx;
    border-bottom-right-radius: 42rpx;
    background-color: #ffffff;
}

/* 查看门票 */
.view_tickets{
    line-height: 84rpx;
