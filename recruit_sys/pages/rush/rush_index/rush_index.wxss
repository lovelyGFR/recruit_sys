/* 大容器 */
page{
    background-color: #F6F6F6;
}
.container1{
   
    height:100%;
    width: 100%;
    font-family: "PingFang SC";
    display: flex;
    flex-direction: column; 
    justify-content: space-between;
    /* align-content: center;      */
    padding:  0;
    background-color: #F6F6F6;
}



/* head_and_body */
.head_and_body{
    height:100%;
    width: 100%;
    font-family: "PingFang SC";
    display: flex;
    flex-direction: column;
    align-content: center;     
    padding:  0;
    background-color: #F6F6F6;
}

/* 云顶书院19届招生宣讲会 */
.container1 .click_details{
    height: 240rpx;
    width: 100%;
    opacity: 0.9;
}
.box{
    display: flex;
    justify-content : space-between;
    flex-direction: row;
    height: 240rpx;
    width: 100%;
    background-color: #fff;
}
.superbox{
    padding: 0 0 0 20rpx;
    display: flex;
    flex-direction: column;
}
.box1{
    font-size: 40rpx;
    color: #222222;
    margin-top: 64rpx;
    letter-spacing:1px
}
.box2{
    font-size: 24rpx;
    color: #888888; 
    margin-top: 38rpx;
    letter-spacing:1px
}
.box3{
    display: flex;
    flex-direction: column;
    justify-content :center;
    width: 40rpx;
    height: 240rpx;
}
image{
    width: 10rpx;
    height: 18rpx;
}

/* 选择场次 */
.time{
    display: flex;
    flex-direction: column;
    justify-content : flex-start;
    height: 200rpx;
    width: 710rpx;
    margin: 30rpx 20rpx;
    border-radius:12rpx;
    background-color: #fff;
}
.time .superbox1{
    display: flex;
    flex-direction: column;
    justify-content : flex-end;
    align-items: center;
    height: 56rpx;
    width: 710rpx;
    font-size: 24rpx;
}
.time .superbox2{
    width: 650rpx;
    height: 74rpx;
    border-radius: 10rpx;
    margin: 30rpx;
    background-color: #F6F6F6;
    display: flex;
    flex-direction: row;
    justify-content: start;
    align-items: center;
    font-size: 30rpx;
    color :#353535;
}

